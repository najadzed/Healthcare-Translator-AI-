{
  "version": 2,
  "builds": [
    { "src": "api/translate.js", "use": "@vercel/node" },
    { "src": "api/health.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "src/(.*)", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/src/(.*)", "dest": "/src/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
